{
    "name": "nepali-datetime",
    "version": "0.1.0",
    "description": "A Nepali date library for extracting dates based on Bikram Sambat (B. S.) and perform converstions with Gregorian calendar (A. D.)",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "require": "./dist/index.js",
            "import": "./dist/index.mjs"
        },
        "./dist/index.js": "./dist/index.js",
        "./dist/index.mjs": "./dist/index.mjs",
        "./dist/index.d.ts": "./dist/index.d.ts"
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "npm run clean && rollup -c",
        "clean": "rimraf dist",
        "test": "jest"
    },
    "repository": {
        "url": "https://github.com/opensource-nepal/node-nepali-datetime.git",
        "type": "git"
    },
    "author": "OpenSource Nepal",
    "license": "MIT",
    "devDependencies": {
        "@rollup/plugin-commonjs": "^25.0.0",
        "@rollup/plugin-node-resolve": "^15.0.2",
        "@rollup/plugin-typescript": "^11.1.1",
        "@types/jest": "^29.5.1",
        "@types/node": "^18.14.1",
        "jest": "^29.5.0",
        "rimraf": "^4.1.2",
        "rollup": "^3.17.2",
        "ts-jest": "^29.1.0",
        "tslib": "^2.5.0",
        "typescript": "^4.9.5"
    }
}
